on boot
    chmod 0660 /proc/touchscreen/double_swipe_enable
    chmod 0660 /proc/touchscreen/down_arrow_enable
    chmod 0660 /proc/touchscreen/down_swipe_enable
    chmod 0660 /proc/touchscreen/left_arrow_enable
    chmod 0660 /proc/touchscreen/left_swipe_enable
    chmod 0660 /proc/touchscreen/letter_c_enable
    chmod 0660 /proc/touchscreen/letter_m_enable
    chmod 0660 /proc/touchscreen/letter_o_enable
    chmod 0660 /proc/touchscreen/letter_s_enable
    chmod 0660 /proc/touchscreen/letter_v_enable
    chmod 0660 /proc/touchscreen/letter_w_enable
    chmod 0660 /proc/touchscreen/letter_z_enable
    chmod 0660 /proc/touchscreen/right_arrow_enable
    chmod 0660 /proc/touchscreen/right_swipe_enable
    chmod 0660 /proc/touchscreen/up_arrow_enable
    chmod 0660 /proc/touchscreen/up_swipe_enable
    chmod 0660 /proc/touchscreen/single_tap_enable
    chmod 0660 /proc/touchscreen/double_tap_enable
    chown system system /proc/touchscreen/double_swipe_enable
    chown system system /proc/touchscreen/down_arrow_enable
    chown system system /proc/touchscreen/down_swipe_enable
    chown system system /proc/touchscreen/left_arrow_enable
    chown system system /proc/touchscreen/left_swipe_enable
    chown system system /proc/touchscreen/letter_c_enable
    chown system system /proc/touchscreen/letter_m_enable
    chown system system /proc/touchscreen/letter_o_enable
    chown system system /proc/touchscreen/letter_s_enable
    chown system system /proc/touchscreen/letter_v_enable
    chown system system /proc/touchscreen/letter_w_enable
    chown system system /proc/touchscreen/letter_z_enable
    chown system system /proc/touchscreen/right_arrow_enable
    chown system system /proc/touchscreen/right_swipe_enable
    chown system system /proc/touchscreen/up_arrow_enable
    chown system system /proc/touchscreen/up_swipe_enable
    chown system system /proc/touchscreen/single_tap_enable
    chown system system /proc/touchscreen/double_tap_enable

service touch-hal-1-0 /system/bin/hw/lineage.touch@1.0-service.asus_Z01R
    interface vendor.lineage.touch@1.0::IGloveMode default
    interface vendor.lineage.touch@1.0::ITouchscreenGesture default
    class hal
    user system
    group system
